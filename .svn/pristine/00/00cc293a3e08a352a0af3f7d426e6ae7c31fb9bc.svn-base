(function () {

    //后退一步
    $('.history').click(function () {
        history.go(-1);
    });


//    类目
    $('.sC-box-ul').find('li').each(function (index,item) {
        $(item).click(function () {
           var _this=$(this);
            _this.siblings().removeClass('li-active');
            _this.addClass('li-active')
        })
    })




})();

