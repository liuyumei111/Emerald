html,
body {
  background: #fff;
}
header {
  position: relative;
  width: 100%;
  height: 46.875px;
  background: #fff;
  border-bottom: 1px solid #d1d1d1;
}
header > span {
  width: 27.77777778px;
  height: 20.83333333px;
  background: url("../images/fanhui.png") no-repeat;
  background-size: 100% 100%;
  display: inline-block;
  position: absolute;
  top: 12.15277778px;
  left: 13.88888889px;
}
.sC-box {
  padding: 0 9.72222222px 0 0;
  width: 100%;
  display: flex;
}
.sC-box .sC-box-ul {
  width: 35%;
  background: #eeeeee;
  z-index: 5;
  position: relative;
}
.sC-box .sC-box-ul > li span {
  margin: 0 10.41666667px;
  text-align: center;
  padding-left: 1.73611111px;
  line-height: 61.11111111px;
  border-bottom: 1px solid #bcbcbc;
  font-size: 15.97222222px;
  display: inline-block;
  width: 80%;
}
.sC-box .sC-box-ul > li span > i {
  width: 10.41666667px;
  height: 10.41666667px;
  border-top: 1px solid #60656b;
  border-right: 1px solid #60656b;
  transform: rotate(45deg);
  display: inline-block;
}
.sC-box .sC-box-ul > .li-active {
  background: #fff;
}
.sC-box .sC-box-ul > .li-active span {
  border: none;
}
.sC-box .sC-box-ol {
  width: 65%;
  display: flex;
  flex-wrap: wrap;
  height: 100%;
}
.sC-box .sC-box-ol > li {
  text-align: center;
  width: 30%;
  background: #eeeeee;
  margin-left: 3.25%;
  font-size: 12.5px;
  line-height: 34.72222222px;
  margin-top: 13.88888889px;
}
.sC-box .sC-box-ul-bg {
  width: 34%;
  height: 100%;
  background: #eeeeee;
  position: absolute;
  top: 46.875px;
  left: 0;
  z-index: 1;
}
