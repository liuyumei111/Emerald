<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>搜索列表</title>
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <link rel="stylesheet" href="css/reset.css">
    <link rel="stylesheet" href="css/swiper-3.4.2.min.css">
    <link rel="stylesheet" href="css/shopRecommend.css">
</head>
<body>
<!--头部-->
<header>
    <h1>搜索列表</h1>
    <span class="history"></span>
    <!--<i class="choiceTarget"></i>-->
</header>
<!--主要内容-->
<div class="box searchShop-box">
    <script type="text/x-handlebars-template" id="searchShop-box">
        <!--商品推荐-->
        <div class="newRecommend">
            <ul>
                {{#each list}}
                <a href="commodityDetail.html?productId={{id}}">
                    <li>
                        <div class="shop-img">
                            <img src="{{images}}" alt="">
                        </div>
                        <p>{{name}}</p>
                        <span>{{price}}</span>
                    </li>
                </a>
                {{/each}}
            </ul>
        </div>
    </script>
    <!--页面加载中-->
    <div class="loader">
        <span>页面加载中
        <i></i></span>
    </div>
    <!--没有更多数据了-->
    <div class="no-info"><span>没有更多数据了</span></div>
</div>

<!--引入js文件-->
<script src="js/zepto.min.js"></script>
<script src="js/swiper-3.4.2.min.js"></script>
<script src="js/config.js"></script>
<script src="js/handlebars-v4.0.10.js"></script>
<script src="js/common.js"></script>
<script src="js/searchShop.js"></script>

</body>
</html>